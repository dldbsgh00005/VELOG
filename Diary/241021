# 241021(월) Javascript 기초 문법 (얄팍한 코딩사전 js 기초 문법편)

arr = ['a','b','c']

for(i in arr){} // 0,1,2
for(i of arr){} // 'a','b','c'

# Axios 활용 방법 1

axios.get({'[URL]'}
.then(function(response)){
  console.log(response)
}
.catch(function(error)){
  console.log(error)
})

# Axios 활용 방법 2

axios.get({
"type" : "get",
"url" : "[URL]",
"params" : {
    "[KEY]" : "[VALUE]",
  }
}
.then(function(response)){
  console.log(response)
}
.catch(function(error)){
  console.log(error)
})


